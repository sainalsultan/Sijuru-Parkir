<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
<!--    <style name="Theme.SijuruParkir" parent="Theme.MaterialComponents.DayNight.DarkActionBar">-->
    <style name="Theme.SijuruParkir" parent="Theme.AppCompat.Light.NoActionBar">
    <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/asapmedium</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Theme.SijuruParkir.Material" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorBlackMinus2</item>
        <item name="colorPrimaryDark">@color/primaryDark</item>
    </style>

